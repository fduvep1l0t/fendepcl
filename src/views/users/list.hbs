<div class="container p-4">
    <div class="row">

        {{#each users}}
            <div class="col-md-3">
                <div class="card text-center">
                    <div class="card-body">
                        {{!--UTILIZAR A HREF PARA POSIBLE REDIRECCION A ESTADISTICAS DE USUARO/TEAM--}}
                        <a href="{{url}}" target="_blank">
                            <h3 class="card-title text-uppercase">
                                {{user_acc}}
                            </h3>
                        </a>
                        <p>{{timeago create_date}}</p>

                        <a href="/users/delete/{{id_user}}" class="btn btn-danger">
                            Eliminar Usuario
                        </a>
                        <a href="/users/edit/{{id_user}}" class="btn btn-secondary">
                            Editar Usuario
                        </a>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
</div>